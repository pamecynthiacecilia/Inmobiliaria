@model Inmobiliaria.Models.Contrato

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Contrato</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
         Inicio del Contrato
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaInicio)
        </dd>
        <dt class="col-sm-2">
          Fin del Contrato
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaFin)
        </dd>
        <dt class="col-sm-2">
            Nro Contrato
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class="col-sm-2">
            Importe mensual
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Importe)
        </dd>
        <dt class="col-sm-2">
            Apellido Inquilino
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.InquilinoContrato.Apellido)

        </dd>
        <dt class="col-sm-2">
            Nombre Inquilino
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.InquilinoContrato.Nombre)

        </dd>
        <dt class="col-sm-2">
            Tipo de Inmueble
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.InmuebleContrato.Tipo)
        </dd>
        <dt class="col-sm-2">
            Direccion
        </dt>
        <dd class="col-sm-10">

            @Html.DisplayFor(model => model.InmuebleContrato.Direccion)
        </dd>

    </dl>
</div>
<div>
    @Html.ActionLink("Editar", "Edit", new {  id = Model.Id }) |
    <a asp-action="Index">Volver</a>
</div>
