@model Inmobiliaria.Models.Pago

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Pago</h4>
    <hr />
    <dl class="row">



        <dt class="col-sm-2">
            Nro de Pago
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NroPago)
        </dd>
        <dt class="col-sm-2">
            Fecha Pago
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FechaP)
        </dd>
        <dt class="col-sm-2">
            Monto Abonado
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Importe)
        </dd>
        <dt class="col-sm-2">
            Nro Contrato
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdContrato)
        </dd>
