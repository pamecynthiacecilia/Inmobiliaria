        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContratoPago.InmuebleContrato.Tipo)
        </dd>
        <dt class="col-sm-2">Dirección</dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ContratoPago.InmuebleContrato.Direccion)
